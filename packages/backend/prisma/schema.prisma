// Prisma Client はMac(darwin-arm64)上で生成されたが、実際の実行環境は(Linuxlinux-arm64-openssl-3.0.x)のため実行時に必要なバイナリが見つからなかった
generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
}